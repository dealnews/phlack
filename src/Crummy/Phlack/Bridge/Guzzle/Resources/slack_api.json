{
    "name": "Slack API",
    "apiVersion": "v1",
    "description": "Slack API",
    "baseUrl": "https://slack.com",
    "operations": {
        "ListChannels": {
            "httpMethod": "GET",
            "uri": "/api/channels.list",
            "summary": "Get the list of channels",
            "responseClass":"ListChannelsOutput",
            "parameters": {
                "token": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "pretty": {
                    "location": "query",
                    "type":     "integer",
                    "required": false,
                    "default": 0
                }
            },
            "additionalParameters": {
                "location": "query"
            }
        },
        "ListGroups": {
            "httpMethod": "GET",
            "uri": "/api/groups.list",
            "summary": "Get the list of groups",
            "responseClass":"ListGroupsOutput",
            "parameters": {
                "token": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "pretty": {
                    "location": "query",
                    "type":     "integer",
                    "required": false,
                    "default": 0
                }
            },
            "additionalParameters": {
                "location": "query"
            }
        },
        "ListIM": {
            "httpMethod": "GET",
            "uri": "/api/im.list",
            "summary": "Get the list of instant message channels",
            "responseClass":"ListIMOutput",
            "parameters": {
                "token": {
                    "location": "query",
                    "type": "string",
                    "required": true
                }
            },
            "additionalParameters": {
                "location": "query"
            }
        },
        "ListUsers": {
            "httpMethod": "GET",
            "uri": "/api/users.list",
            "summary": "Get the list of users",
            "responseClass":"ListUsersOutput",
            "parameters": {
                "token": {
                    "location": "query",
                    "type": "string",
                    "required": true
                }
            },
            "additionalParameters": {
                "location": "query"
            }
        }
    },
