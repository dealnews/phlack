                    "type": "string"
                },
                "created": {
                    "location": "json",
                    "type": "float"
                },
                "creator": {
                    "location": "json",
                    "type": "number"
                },
                "is_archived": {
                    "location": "json",
                    "type": "boolean"
                },
                "is_member": {
                    "location": "json",
                    "type": "boolean"
                },
                "num_members": {
                    "location": "json",
                    "type": "integer"
                },
                "topic": {
                    "$ref": "Topic"
                },
                "purpose": {
                    "$ref": "Topic"
                }
            }
        },
        "Group": {
            "type": "object",
            "properties": {
                "id": {
